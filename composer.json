{
    "name": "sheum/laravel-auto-transaction",
    "description": "Automatic database transaction management for Laravel applications",
    "keywords": ["laravel", "database", "transaction", "auto-commit", "rollback"],
    "license": "MIT",
    "authors": [
        {
            "name": "MD Nazmul Hasan Sheum",
            "email": "nazmulhasansheum@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/http": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0",
        "mockery/mockery": "^1.5",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Sheum\\AutoTransaction\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Sheum\\AutoTransaction\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sheum\\AutoTransaction\\AutoTransactionServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
